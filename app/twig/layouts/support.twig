<div class="popup-wrapper js-popup" data-popup="technical-support">
    <div class="popup">
        <section class="header header_bg mb25">
            <div class="container container_pink">
                <div class="header-flex header-flex_popup">
                    <i class="icon-left pointer js-close-popup"></i>
                    <div class="header__logo">
                        <p class="logo-title text-white fz36 text-center">Denim</p>
                    </div>
                </div>
            </div>
        </section>
        <section class="technical-support">
            <div class="container">
                <p class="fz16 fw700 mb35">Обращение в службу поддержки</p>
                <form class="support-form" id="support-form" action="#" method="get">
                    <div class="support-form__subject gray-text js-open-subject js-subject-field mb30">Укажите тему
                    </div>
                    <div class="subject-wrapper js-subject">
                        <div class="modal-subject">
                            <p class="fz18 fw700 mb40">Укажите тему</p>
                            <div class="subject-form">
                                <div class="js-radiobox radiobox">
                                    <div class="radiobox__radio fz16">
                                        <input type="radio" name="group1" id="install">
                                        <label class="radiobox__radio-label" for="install">Установка приложения</label>
                                    </div>
                                    <div class="radiobox__radio fz16">
                                        <input type="radio" name="group1" id="error">
                                        <label class="radiobox__radio-label" for="error">Ошибка в работе
                                            приложения</label>
                                    </div>
                                </div>
                                <div class="radiobox__buttons fz16">
                                    <span class="radiobox__button js-submit-subject mr15">ОК</span>
                                    <span class="radiobox__button js-close-subject">Отмена</span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <input class="support-form__input mb30" name="email" type="email" placeholder="Email">
                    <textarea class="support-form__textarea js-auto-size mb22" placeholder="Опишите вашу проблему"
                              rows="1"></textarea>
                    <p class="text-center">
                        <button class="button button_gray button-support" for="support-form" type="submit">Отправить
                        </button>
                    </p>
                </form>
            </div>
        </section>
    </div>
</div>

<div class="popup-wrapper js-popup" data-popup="instruction">
